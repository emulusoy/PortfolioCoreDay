@model List<Skill>
<section id="skills" class="skills section light-background">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Skills</h2>
        <p>You can see my skills on this page</p>
    </div><!-- End Section Title -->

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row skills-content skills-animation">

            <div class="col-lg-12">
                @foreach (var item in Model)
                {
                    
              
                <div class="progress">
                    <span class="skill"><span>@item.SkillName</span> <i class="val">@item.SkillValue%</i></span>
                    <div class="progress-bar-wrap">
                            <div class="progress-bar" role="progressbar" aria-valuenow="@item.SkillValue" aria-valuemin="0" aria-valuemax="@item.SkillValue"></div>
                    </div>
                </div>
                }

            </div>
        </div>

    </div>

</section>
